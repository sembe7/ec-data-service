{
  "presets": [
    "@babel/preset-env",
    "@babel/react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": false
      }
    ],
    [
      "@babel/transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
